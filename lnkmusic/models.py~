from django.db import models

class PBA(models.Model):
    name = models.CharField(max_length=255)
    name_slug = models.SlugField()
   
class Bourbon(models.Model):
    name = models.CharField(max_length=255)
    name_slug = models.SlugField()
    
class Vega(models.Model):
    name = models.CharField(max_length=255)
    name_slug = models.SlugField()
   
class SingleBarrel(models.Model):
    name = models.CharField(max_length=255)
    name_slug = models.SlugField()
    


